Algoritmo Porcenaje
	definir hombre Como Real;
	definir mujeres Como Real;
	
	Escribir "escribe el total de hombre"
	Leer  hombre;
	
	Escribir "Escribe el total de mujeres"
	Leer  mujeres
	
	total = hombre + mujeres
	
	Escribir  "el total de alumnos es: " total;
	escribir "el porcentaje de hombres es: " (hombre/total)*100;
	escribir "el porcentaje de mujeres es: " (mujeres/total)*100;
	
FinAlgoritmo
